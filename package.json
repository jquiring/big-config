{
  "name": "big-config",
  "version": "3.9.0",
  "description": "Easily manage configuration settings for small to very large projects",
  "repository": "natesilva/big-config",
  "license": "BSD-3-Clause",
  "author": "Nate Silva <nate@natesilva.com>",
  "keywords": [
    "conf",
    "config",
    "configuration",
    "env",
    "environment",
    "json",
    "yaml"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "clean": "del-cli build",
    "build": "tsc",
    "prettier": "prettier --write \"{src,test}/**/*.?(ts|json)\"",
    "test": "mocha",
    "test:watch": "mocha -w",
    "coverage": "nyc --reporter lcov --reporter text --extension .ts mocha",
    "prepublishOnly": "tsc",
    "postpublish": "del-cli build"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.160",
    "@types/mocha": "^8.0.3",
    "@types/node": "^10.17.28",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^8.1.2",
    "np": "^6.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "testdouble": "^3.16.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "lodash": "^4.17.20",
    "type-fest": "^0.16.0"
  },
  "volta": {
    "node": "10.22.0",
    "npm": "6.14.8"
  }
}
